{
    "name": "dobryak/phpsyslognet",
    "description": "PHP Syslog client that conforms to the RFC5424 syslog standard and supports TCP/UDP transports",
    "keywords": [
        "syslog",
        "rfc5424",
        "syslog-tcp",
        "syslog-udp",
        "syslog-unix"
    ],
    "license": "GNU GPLv3",
    "authors": [
        {
            "name": "Dobriakov Anton",
            "email": "anton.dobryakov@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6",
        "ext-sockets": "*",
        "ext-ctype": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "SyslogNet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SyslogNet\\Tests\\": "tests/"
        }
    }
}
